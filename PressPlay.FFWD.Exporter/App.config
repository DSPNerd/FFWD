<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="PressPlay">
      <section name="FFWD" type="PressPlay.FFWD.Exporter.Configuration.ConfigurationSectionHandler,PressPlay.FFWD"/>
    </sectionGroup>
  </configSections>
  <PressPlay>
    <FFWD>
      <TypeResolver ExcludeByDefault="false">
        <ExcludeTypes>
          <Type>UnityEngine.Transform</Type>
        </ExcludeTypes>
        <NamespaceRules>
          <Convert Namespace="UnityEngine" To="PressPlay.FFWD.Components"/>
          <Convert Type="XNAMeshCollider" To="PressPlay.FFWD.Components.MeshCollider"/>
          <!--<Convert Type="PackedSprite" To="PressPlay.FFWD.Components.AnimatedQuadRenderer"/>-->
        </NamespaceRules>
        <ComponentWriters>
          <Map Type="UnityEngine.MeshRenderer" To="PressPlay.FFWD.Exporter.Writers.Components.MeshRendererWriter, PressPlay.FFWD.Exporter"/>
          <Map Type="UnityEngine.SkinnedMeshRenderer" To="PressPlay.FFWD.Exporter.Writers.Components.MeshRendererWriter, PressPlay.FFWD.Exporter"/>
          <Map Type="UnityEngine.BoxCollider" To="PressPlay.FFWD.Exporter.Writers.Components.BoxColliderWriter, PressPlay.FFWD.Exporter"/>
          <Map Type="UnityEngine.SphereCollider" To="PressPlay.FFWD.Exporter.Writers.Components.SphereColliderWriter, PressPlay.FFWD.Exporter"/>
          
          <!-- TENTACLES SPECIFIC STUFF -->
          <Map Type="XNAMeshCollider" To="PressPlay.FFWD.Exporter.Writers.Components.Extensions.XNAMeshColliderWriter, PressPlay.FFWD.Exporter"/>
          <!--<Map Type="PackedSprite" To="PressPlay.FFWD.Exporter.Writers.Components.Extensions.PackedSpriteWriter, PressPlay.FFWD.Exporter"/>-->
          <Map Type="SimpleRotate" To="PressPlay.FFWD.Exporter.Writers.Components.MonoBehaviourWriter, PressPlay.FFWD.Exporter"/>
          <Map Type="LevelHandler" To="PressPlay.FFWD.Exporter.Writers.Components.ManualScriptWriter, PressPlay.FFWD.Exporter"/>
          <Map Type="LevelTypeSettings" To="PressPlay.FFWD.Exporter.Writers.Components.ManualScriptWriter, PressPlay.FFWD.Exporter"/>
          <Map Type="AnimatedCheckPoint" To="PressPlay.FFWD.Exporter.Writers.Components.ManualScriptWriter, PressPlay.FFWD.Exporter"/>
        </ComponentWriters>
      </TypeResolver>
    </FFWD>
  </PressPlay>
<startup><supportedRuntime version="v2.0.50727"/></startup></configuration>
